{
    "manifest_version": 2,
    "name": "tabzap",
    "version": "1.5",
    "background": {
        "scripts":[
            "config.js",
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "icon48.png"
    },

    "options_ui": {
        "page": "options.html"
    },

    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": [
        "tabs", "notifications", "webNavigation", "storage"
    ],
    "web_accessible_resources": [
        "icon48.png"
    ]
}